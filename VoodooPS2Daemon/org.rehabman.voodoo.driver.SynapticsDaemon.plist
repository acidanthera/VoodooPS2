<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>org.rehabman.voodoo.driver.SynapticsDaemon</string>
	<key>KeepAlive</key>
	<true/>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardOutPath</key>
	<string>/var/log/VoodooPS2SynapticsDaemon.log</string>
	<key>StandardErrorPath</key>
	<string>/var/log/VoodooPS2SynapticsDaemon.log</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/VoodooPS2Daemon</string>
	</array>
</dict>
</plist>
